{
    "expo": {
        "name": "mosh-react-native",
        "slug": "mosh-react-native",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./src/assets/icon.png",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./src/assets/favicon.png"
        },
        "description": "Working through the React Native Series course from Mosh."
    }
}